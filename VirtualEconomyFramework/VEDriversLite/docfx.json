{
  "metadata": [
    {
      "src": [
        {
          "src": "..",
          "files": [
            "VEDriversLite/**.csproj",
            "VEDriversLite.NeblioAPI/VEDriversLite.NeblioAPI.csproj",
            "VEDriversLite.Extensions.WooCommerce/VEDriversLite.Extensions.WooCommerce.csproj",
            "VEDriversLite.Cryptocurrencies/VEDriversLite.Cryptocurrencies.csproj",
             "VEDriversLite.Builders.Neblio/VEDriversLite.Builders.Neblio.csproj"
          ]
          
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template":["default","templates/discordfx"],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
